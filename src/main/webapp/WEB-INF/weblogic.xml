<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-web-app" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://xmlns.oracle.com/weblogic/weblogic-web-app http://xmlns.oracle.com/weblogic/weblogic-web-app/1.4/weblogic-web-app.xsd">
    <wls:context-root>first-sample</wls:context-root>
    <wls:library-ref>
        <wls:library-name>jersey-bundle</wls:library-name>
        <wls:specification-version>1.1.1</wls:specification-version>
        <wls:implementation-version>1.1.5.1</wls:implementation-version>
        <wls:exact-match>false</wls:exact-match>
    </wls:library-ref>
    <wls:library-ref>
        <wls:library-name>jsr311-api</wls:library-name>
        <wls:specification-version>1.1.1</wls:specification-version>
        <wls:implementation-version>1.1.1</wls:implementation-version>
        <wls:exact-match>false</wls:exact-match>
    </wls:library-ref>
    <wls:library-ref>
        <wls:library-name>jackson-core-asl</wls:library-name>
        <wls:specification-version>1.0</wls:specification-version>
        <wls:implementation-version>1.1.1</wls:implementation-version>
        <wls:exact-match>false</wls:exact-match>
    </wls:library-ref>

    <wls:container-descriptor>
        <wls:prefer-application-packages>
            <wls:package-name>org.apache.openjpa.*</wls:package-name>
        </wls:prefer-application-packages>
    </wls:container-descriptor>

</wls:weblogic-web-app>